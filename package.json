{
  "name": "travesty",
  "version": "0.1.0",
  "description": "Simple webapp for turning possibly excellent prose into drivel",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint ./src/*.js; true",
    "eslint-test": "eslint ./test/spec/**.js; true",
    "jshint": "jshint ./src/*.js; true",
    "jshint-test": "jshint ./test/spec/**.js; true",
    "check": "npm run jshint && npm run jshint-test && npm run eslint && npm run eslint-test",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javazen/travesty.git"
  },
  "keywords": [
    "ES6",
    "CodeKit",
    "lodash"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "mocha": "^8.4.0",
    "requirejs": "^2.3.6"
  }
}
