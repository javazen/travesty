{
  "name": "travesty",
  "version": "0.1.0",
  "description": "Simple webapp for turning possibly excellent prose into drivel",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint ./src/*.js; true",
    "eslint-test": "eslint ./test/spec/**.js; true",
    "jshint": "jshint ./src/*.js; true",
    "jshint-test": "jshint ./test/spec/**.js; true",
