    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javazen/travesty.git"
  },
  "keywords": [
    "github",
    "ES6 module"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.0.0",
    "mocha": "^9.0.0",
    "requirejs": "^2.3.6"
  }
}
